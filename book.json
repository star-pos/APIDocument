{

    "title": "开发者文档",
    "description": "新大陆星POS开发者文档",
    "author": "He Qian",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "plugins" : [ 
        "-share",
        "-lunr", 
        "-search", 
        "search-pro",
        "book-summary-scroll-position-saver",
        "expandable-chapters-small",
        
        "simple-page-toc",
        "anchors",
        "emphasize",
        "splitter",
        "alerts",
        "prism",
        "-highlight",
        "sitemap",
        "favicon"
    ],
    "pluginsConfig": {
    
        "expandable-chapters-small":{},
        
         "atoc": {
            "addClass": true,
            "className": "atoc"
        },
        "sitemap": {
            "hostname": "http://starpos.com.cn/"
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        },
        "prism": {
            "css": [
                "prism-themes/themes/prism-base16-ateliersulphurpool.light.css"
            ]
        },
        "favicon": {
            "shortcut": "assets/favicon.ico",
            "bookmark": "assets/favicon.ico",
            "appleTouch": "assets/favicon.ico",
            "appleTouchMore": {
                "120x120": "assets/favicon.ico",
                "180x180": "assets/favicon.ico"
            }
        }
    },
    "styles": {
          "website": "website.css"
    }
}